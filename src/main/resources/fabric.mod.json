{
  "schemaVersion": 1,
  "id": "squished",
  "version": "${version}",
  "name": "Squished",
  "description": "Allows gravity affected blocks to squish you when they fall on you!",
  "authors": [
    "Mayaqq"
  ],
  "contact": {
    "repo": "https://github.com/MayaqqDev/Squished"
  },
  "license": "LGPL-3.0",
  "icon": "assets/squished/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "dev.mayaqq.squished.client.SquishedClient"
    ],
    "main": [
      "dev.mayaqq.squished.Squished"
    ],
    "fabric-datagen": [
      "dev.mayaqq.squished.datagen.SquishedDatagen"
    ]
  },
  "mixins": [
    "squished.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "pehkui": "*"
  }
}
